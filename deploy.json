{
	"containers": [{
		"image": "{{.IMAGE}}",
		"name": "{{.NAME}}-{{.HASH}}",
		"limits": {
			"cpu": {{.CPU}},
			"memory": {{.RAM}}
		},
		"env": [{
				"name": "POSTGRES_USER",
				"value": "{{.USER}}"
			},
			{
				"name": "POSTGRES_PASSWORD",
				"value": "{{.PASSWORD}}"
			},
			{
				"name": "PGDATA",
				"value": "{{.DATA}}"
			}
		]
	}],
	"name": "{{.NAME}}-{{.HASH}}",
	"replicas": 1
}